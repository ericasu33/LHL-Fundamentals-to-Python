steps = [
  "turn left",
  "go straight for 2 blocks",
  "turn right",
  "keep going until you see the dog statue",
  "turn right",
  "turn right again",
  "Park right on the sidewalk"
]

instructioncap = []

instructions = "First,"


for step in steps:
  caps = step.upper()
  instructioncap.append(caps)
  instructions = instructions + instructioncap + ",then"

print("First," + instructioncap +  "you have arrived!")