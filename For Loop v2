steps = [
  "turn left",
  "go straight for 2 blocks",
  "turn right",
  "keep going until you see the dog statue",
  "turn right",
  "turn right again",
  "Park right on the sidewalk"
]

screamedsteps = []



for step in steps:
  caps = step.upper()
  screamedsteps.append(caps)

print(screamedsteps)
